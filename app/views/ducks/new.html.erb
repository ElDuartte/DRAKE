<%= simple_form_for(@duck) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :price %>
  <%= f.text_field :price %>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <%= f.label :photo %>
  <%= f.input :photo, as: :file %>
  <%= f.submit %>
<% end %>
